    ns1:hasCondition ns1:Condition_KhongThuocTruongHop ;
    ns1:isPunishedBy ns1:Punishment_CaiTaoKhongGiamGiữ_03Nam ;
    ns1:isPunishedBy ns1:Punishment_Tu_06ThangDen_03Nam .

ns1:Condition_KhongThuocTruongHop a ns1:Condition .

ns1:Punishment_CaiTaoKhongGiamGiữ_03Nam a ns1:Punishment ;
    ns1:hasText "Phạt cải tạo không giam giữ đến 03 năm" .

ns1:Punishment_Tu_06ThangDen_03Nam a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 06 tháng đến 03 năm" .

ns1:Condition_CoToChuc a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_LoiDungChucVu a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_DoiTuongThiHanhCongVu a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_PhamToi_2LanTroLen a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_DoiTuong_2NguoiTroLen a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_DoiTuongDuoi_18 a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_GiaDinhKhongKhan a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Condition_GayRoiLoanTamThan a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_02Den_07Nam .

ns1:Punishment_Tu_02Den_07Nam a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 02 năm đến 07 năm" .

ns1:Punishment_Tu_05Den_12Nam a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 05 năm đến 12 năm" .

ns1:Condition_LamCheTieu a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_05Den_12Nam .

ns1:Condition_TraTan a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_05Den_12Nam .

ns1:Condition_GayRoiLoanTamThan_46 a ns1:Condition ;
    ns1:isPunishedBy ns1:Punishment_Tu_05Den_12Nam .

ns1:Punishment_CamDamNhiemChucVu a ns1:Punishment ;
    ns1:hasText "Cấm đảm nhiệm chức vụ nhất định từ 01 năm đến 05 năm" .


ns1:Article158 a ns1:LegalProvision ;
    ns1:hasText "Điều 158. Tội xâm phạm chỗ ở của người khác"^^xsd:string .

ns1:Crime_InvasionOfResidence a ns1:Crime ;
    ns1:definedIn ns1:Article158 ;
    ns1:hasCondition ns1:Condition_IllegalSearch ;
    ns1:hasCondition ns1:Condition_UseOfForce ;
    ns1:hasCondition ns1:Condition_IllegalOccupation ;
    ns1:hasCondition ns1:Condition_UnauthorizedEntry ;
    ns1:isPunishedBy ns1:Punishment_CommunityService ; 
    ns1:isPunishedBy ns1:Punishment_Prison .

ns1:Condition_IllegalSearch a ns1:Condition ;
    ns1:hasText "Khám xét trái pháp luật chỗ ở của người khác"^^xsd:string .

ns1:Condition_UseOfForce a ns1:Condition ;
    ns1:hasText "Dùng vũ lực, đe dọa dùng vũ lực, gây sức ép về tinh thần hoặc thủ đoạn trái pháp luật khác buộc người khác phải rời khỏi chỗ ở hợp pháp của họ"^^xsd:string .

ns1:Condition_IllegalOccupation a ns1:Condition ;
    ns1:hasText "Dùng mọi thủ đoạn trái pháp luật nhằm chiếm, giữ chỗ ở hoặc cản trở trái phép, không cho người đang ở hoặc quản lý hợp pháp chỗ ở được vào chỗ ở của họ"^^xsd:string .

ns1:Condition_UnauthorizedEntry a ns1:Condition ;
    ns1:hasText "Tự ý xâm nhập chỗ ở của người khác mà không được sự đồng ý của chủ nhà hoặc người quản lý hợp pháp"^^xsd:string .

ns1:Punishment_CommunityService a ns1:Punishment ;
    ns1:hasText "Phạt cải tạo không giam giữ đến 02 năm"^^xsd:string .

ns1:Punishment_Prison a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 03 tháng đến 02 năm"^^xsd:string . 

ns1:Condition_SevereCircumstances a ns1:Condition ;
    ns1:hasText "Có tổ chức; Lợi dụng chức vụ, quyền hạn; Phạm tội 02 lần trở lên; Làm người bị xâm phạm chỗ ở tự sát; Gây ảnh hưởng xấu đến an ninh, trật tự, an toàn xã hội"^^xsd:string . 

ns1:Punishment_SeverePrison a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 01 năm đến 05 năm"^^xsd:string . 

ns1:Crime_InvasionOfResidence_Severe a ns1:Crime ;
    ns1:definedIn ns1:Article158 ;
    ns1:hasCondition ns1:Condition_SevereCircumstances ;
    ns1:isPunishedBy ns1:Punishment_SeverePrison . 

ns1:Condition_Prohibition a ns1:Condition ;
    ns1:hasText "Cấm đảm nhiệm chức vụ nhất định từ 01 năm đến 05 năm"^^xsd:string . 

ns1:Crime_InvasionOfResidence_Prohibition a ns1:Crime ;
    ns1:definedIn ns1:Article158 ;
    ns1:hasCondition ns1:Condition_Prohibition ;
    ns1:isPunishedBy ns1:Punishment_SeverePrison .


ns1:Article159 a ns1:LegalProvision ;
    ns1:hasText "Điều 159. Tội xâm phạm bí mật hoặc an toàn thư tín, điện thoại, điện tín hoặc hình thức trao đổi thông tin riêng tư khác của người khác" .

ns1:Crime_InvasionOfPrivacy a ns1:Crime ;
    ns1:definedIn ns1:Article159 ;
    ns1:hasCondition ns1:Condition_ViolatingDiscipline ;
    ns1:isPunishedBy ns1:Punishment_WarningFine .

ns1:Condition_ViolatingDiscipline a ns1:Condition ;
    ns1:hasText "Người nào thực hiện một trong các hành vi sau đây, đã bị xử lý kỷ luật hoặc xử phạt vi phạm hành chính về hành vi này mà còn vi phạm" .

ns1:Punishment_WarningFine a ns1:Punishment ;
    ns1:hasText "Bị phạt cảnh cáo, phạt tiền từ 20.000.000 đồng đến 50.000.000 đồng hoặc phạt cải tạo không giam giữ đến 03 năm" .

ns1:Condition_OrganizedCrime a ns1:Condition ;
    ns1:hasText "Có tổ chức" .

ns1:Condition_AbuseOfPower a ns1:Condition ;
    ns1:hasText "Lợi dụng chức vụ, quyền hạn" .

ns1:Condition_RepeatedOffense a ns1:Condition ;
    ns1:hasText "Phạm tội 02 lần trở lên" .

ns1:Condition_InformationDisclosure a ns1:Condition ;
    ns1:hasText "Tiết lộ các thông tin đã chiếm đoạt, làm ảnh hưởng đến danh dự, uy tín, nhân phẩm của người khác" .

ns1:Condition_SuicideRisk a ns1:Condition ;
    ns1:hasText "Làm nạn nhân tự sát" .

ns1:Punishment_Imprisonment a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 01 năm đến 03 năm" .

ns1:Punishment_Fine a ns1:Punishment ;
    ns1:hasText "Bị phạt tiền từ 5.000.000 đồng đến 20.000.000 đồng, cấm đảm nhiệm chức vụ nhất định từ 01 năm đến 05 năm" .


ns1:Article168 a ns1:LegalProvision ;
    ns1:hasText "Điều 168. Tội cướp tài sản" .

ns1:Crime_Robbery a ns1:Crime ;
    ns1:definedIn ns1:Article168 ;
    ns1:hasCondition ns1:Condition_UseViolence ;
    ns1:isPunishedBy ns1:Punishment_3to10Years .

ns1:Condition_UseViolence a ns1:Condition ;
    ns1:hasText "Người nào dùng vũ lực, đe dọa dùng vũ lực ngay tức khắc hoặc có hành vi khác làm cho người bị tấn công lâm vào tình trạng không thể chống cự được nhằm chiếm đoạt tài sản." .

ns1:Punishment_3to10Years a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 03 năm đến 10 năm." .

ns1:Condition_Organized a ns1:Condition ;
    ns1:hasText "Có tổ chức." .

ns1:Condition_Professional a ns1:Condition ;
    ns1:hasText "Có tính chất chuyên nghiệp." .

ns1:Condition_Injury11to30 a ns1:Condition ;
    ns1:hasText "Gây thương tích hoặc gây tổn hại cho sức khỏe của người khác mà tỷ lệ tổn thương cơ thể từ 11% đến 30%." .

ns1:Condition_UseWeapon a ns1:Condition ;
    ns1:hasText "Sử dụng vũ khí, phương tiện hoặc thủ đoạn nguy hiểm khác." .

ns1:Condition_Value50to200 a ns1:Condition ;
    ns1:hasText "Chiếm đoạt tài sản trị giá từ 50.000.000 đồng đến dưới 200.000.000 đồng." .

ns1:Condition_VulnerableGroups a ns1:Condition ;
    ns1:hasText "Phạm tội đối với người dưới 16 tuổi, phụ nữ mà biết là có thai, người già yếu hoặc người không có khả năng tự vệ." .

ns1:Condition_NegativeImpact a ns1:Condition ;
    ns1:hasText "Gây ảnh hưởng xấu đến an ninh, trật tự, an toàn xã hội." .

ns1:Condition_Reoffending a ns1:Condition ;
    ns1:hasText "Tái phạm nguy hiểm." .

ns1:Punishment_7to15Years a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 07 năm đến 15 năm." .

ns1:Condition_Value200to500 a ns1:Condition ;
    ns1:hasText "Chiếm đoạt tài sản trị giá từ 200.000.000 đồng đến dưới 500.000.000 đồng." .

ns1:Condition_Injury31to60 a ns1:Condition ;
    ns1:hasText "Gây thương tích hoặc gây tổn hại cho sức khỏe của người khác mà tỷ lệ tổn thương cơ thể từ 31% đến 60%." .

ns1:Condition_ExploitationDisaster a ns1:Condition ;
    ns1:hasText "Lợi dụng thiên tai, dịch bệnh." .

ns1:Punishment_12to20Years a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 12 năm đến 20 năm." .

ns1:Condition_Value500Plus a ns1:Condition ;
    ns1:hasText "Chiếm đoạt tài sản trị giá 500.000.000 đồng trở lên." .

ns1:Condition_Injury61Plus a ns1:Condition ;
    ns1:hasText "Gây thương tích hoặc gây tổn hại cho sức khỏe của 01 người mà tỷ lệ tổn thương cơ thể 61% trở lên hoặc gây thương tích hoặc gây tổn hại cho sức khỏe của 02 người trở lên mà tỷ lệ tổn thương cơ thể của mỗi người 31% trở lên." .

ns1:Condition_CausingDeath a ns1:Condition ;
    ns1:hasText "Làm chết người." .

ns1:Condition_ExploitationWar a ns1:Condition ;
    ns1:hasText "Lợi dụng hoàn cảnh chiến tranh, tình trạng khẩn cấp." .

ns1:Punishment_18to20YearsOrLife a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 18 năm đến 20 năm hoặc tù chung thân." .

ns1:Punishment_1to5Years a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 01 năm đến 05 năm." .

ns1:Punishment_Fines a ns1:Punishment ;
    ns1:hasText "Có thể bị phạt tiền từ 10.000.000 đồng đến 100.000.000 đồng, phạt quản chế, cấm cư trú từ 01 năm đến 05 năm hoặc tịch thu một phần hoặc toàn bộ tài sản." .


ns1:Article161 a ns1:LegalProvision ;
    ns1:hasText "Điều 161. Tội làm sai lệch kết quả bầu cử, kết quả trưng cầu ý dân"^^xsd:string .

ns1:Crime_DistortingElectionResults a ns1:Crime ;
    ns1:definedIn ns1:Article161 ;
    ns1:hasCondition ns1:Condition_OrganizationalResponsibility ;
    ns1:isPunishedBy ns1:Punishment_UpTo2Years .

ns1:Condition_OrganizationalResponsibility a ns1:Condition ;
    ns1:hasText "Người nào có trách nhiệm trong việc tổ chức, giám sát việc bầu cử, tổ chức trưng cầu ý dân mà giả mạo giấy tờ, gian lận phiếu hoặc dùng thủ đoạn khác để làm sai lệch kết quả bầu cử, kết quả trưng cầu ý dân."^^xsd:string .

ns1:Punishment_UpTo2Years a ns1:Punishment ;
    ns1:hasText "Bị phạt cải tạo không giam giữ đến 02 năm hoặc phạt tù từ 03 tháng đến 02 năm."^^xsd:string .

ns1:Condition_SevereCircumstances a ns1:Condition ;
    ns1:hasText "Phạm tội thuộc một trong các trường hợp sau đây, thì bị phạt tù từ 01 năm đến 03 năm: a) Có tổ chức; b) Dẫn đến phải tổ chức lại việc bầu cử hoặc trưng cầu ý dân."^^xsd:string .

ns1:Punishment_1To3Years a ns1:Punishment ;
    ns1:hasText "Bị phạt tù từ 01 năm đến 03 năm."^^xsd:string .

ns1:Punishment_BanFromPosition a ns1:Punishment ;
    ns1:hasText "Người phạm tội còn có thể bị cấm đảm nhiệm chức vụ nhất định từ 01 năm đến 05 năm."^^xsd:string .



ns1:Article162 a ns1:LegalProvision ;
    ns1:hasText "Điều 162. Tội buộc công chức, viên chức thôi việc hoặc sa thải người lao động trái pháp luật" .

ns1:Crime_IllegalDismissal a ns1:Crime ;
    ns1:definedIn ns1:Article162 ;
    ns1:hasCondition ns1:Condition_ProfitMotive ;
    ns1:isPunishedBy ns1:Punishment_FineOrImprisonment .

ns1:Condition_ProfitMotive a ns1:Condition ;
    ns1:hasText "Người nào vì vụ lợi hoặc động cơ cá nhân khác mà thực hiện một trong các hành vi sau đây làm cho người bị thôi việc, người bị sa thải hoặc gia đình họ lâm vào tình trạng khó khăn hoặc dẫn đến đình công." .

ns1:Punishment_FineOrImprisonment a ns1:Punishment ;
    ns1:hasText "Bị phạt tiền từ 10.000.000 đồng đến 100.000.000 đồng, phạt cải tạo không giam giữ đến 01 năm hoặc phạt tù từ 03 tháng đến 01 năm." .

ns1:Condition_MultipleOffenders a ns1:Condition ;
    ns1:hasText "Đối với 02 người trở lên." .

ns1:Condition_PregnantWomen a ns1:Condition ;
    ns1:hasText "Đối với phụ nữ mà biết là có thai." .

ns1:Condition_ChildCare a ns1:Condition ;
    ns1:hasText "Đối với người đang nuôi con dưới 12 tháng tuổi." .

ns1:Condition_Suicide a ns1:Condition ;
    ns1:hasText "Làm người bị buộc thôi việc, người bị sa thải tự sát." .

ns1:Punishment_HigherFineOrImprisonment a ns1:Punishment ;
    ns1:hasText "Bị phạt tiền từ 100.000.000 đồng đến 200.000.000 đồng hoặc phạt tù từ 01 năm đến 03 năm." .

ns1:Crime_IllegalDismissal a ns1:Crime ;
    ns1:definedIn ns1:Article162 ;
    ns1:hasCondition ns1:Condition_MultipleOffenders ;
    ns1:hasCondition ns1:Condition_PregnantWomen ;
    ns1:hasCondition ns1:Condition_ChildCare ;
    ns1:hasCondition ns1:Condition_Suicide ;
    ns1:isPunishedBy ns1:Punishment_HigherFineOrImprisonment .

ns1:Crime_ProhibitionOfPosition a ns1:Crime ;
    ns1:definedIn ns1:Article162 ;
    ns1:isPunishedBy ns1:Punishment_ProhibitionOfPosition .

ns1:Punishment_ProhibitionOfPosition a ns1:Punishment ;
    ns1:hasText "Bị cấm đảm nhiệm chức vụ nhất định từ 01 năm đến 05 năm." .



ns1:Article163 a ns1:LegalProvision ;
    ns1:hasText "Điều 163. Tội xâm phạm quyền hội họp, lập hội của công dân" .

ns1:Crime_InterferenceWithMeetings a ns1:Crime ;
    ns1:definedIn ns1:Article163 ;
    ns1:hasCondition ns1:Condition_UseOfForce ;
    ns1:isPunishedBy ns1:Punishment_CommunityServiceOrImprisonment .

ns1:Condition_UseOfForce a ns1:Condition ;
    ns1:hasText "Người nào dùng vũ lực, đe dọa dùng vũ lực hoặc dùng thủ đoạn khác ngăn cản hoặc ép buộc người khác lập hội, hội họp hợp pháp." .

ns1:Punishment_CommunityServiceOrImprisonment a ns1:Punishment ;
    ns1:hasText "Phạt cải tạo không giam giữ đến 01 năm hoặc phạt tù từ 03 tháng đến 01 năm." .

ns1:Condition_OrganizedCrime a ns1:Condition ;
    ns1:hasText "Có tổ chức." .

ns1:Condition_AbuseOfPower a ns1:Condition ;
    ns1:hasText "Lợi dụng chức vụ, quyền hạn." .

ns1:Condition_RepeatedOffense a ns1:Condition ;
    ns1:hasText "Phạm tội 02 lần trở lên." .

ns1:Condition_LeadingToProtests a ns1:Condition ;
    ns1:hasText "Dẫn đến biểu tình." .

ns1:Condition_NegativeImpactOnSecurity a ns1:Condition ;
    ns1:hasText "Gây ảnh hưởng xấu đến an ninh, trật tự, an toàn xã hội." .

ns1:Punishment_ImprisonmentForSeriousOffenses a ns1:Punishment ;
    ns1:hasText "Phạt tù từ 01 năm đến 03 năm." .

ns1:Punishment_BanOnPosition a ns1:Punishment ;
    ns1:hasText "Cấm đảm nhiệm chức vụ, cấm hành nghề hoặc làm công việc nhất định từ 01 năm đến 05 năm." .



ns1:Article164 a ns1:LegalProvision ;
    ns1:hasText "Điều 164. Tội xâm phạm quyền tự do tín ngưỡng, tôn giáo của người khác"^^xsd:string .

ns1:Crime_InterferenceWithReligiousFreedom a ns1:Crime ;
    ns1:definedIn ns1:Article164 ;
    ns1:hasCondition ns1:Condition_UseOfForce ;
    ns1:isPunishedBy ns1:Punishment_UpTo1Year ;
    ns1:isPunishedBy ns1:Punishment_3To12Months .

ns1:Condition_UseOfForce a ns1:Condition ;
    ns1:hasText "Người nào dùng vũ lực, đe dọa dùng vũ lực hoặc dùng thủ đoạn khác ngăn cản hoặc ép buộc người khác thực hiện quyền tự do tín ngưỡng, tôn giáo."^^xsd:string .

ns1:Punishment_UpTo1Year a ns1:Punishment ;
    ns1:hasText "Phạt cải tạo không giam giữ đến 01 năm hoặc phạt tù từ 03 tháng đến 01 năm."^^xsd:string .

ns1:Punishment_3To12Months a ns1:Punishment ;
